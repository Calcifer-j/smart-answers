<% outcome = @presenter.current_node %>
<% content_for :outcome_title do %>
  <% if outcome.title.present? %><%= outcome.title %><% else %>Outcome<% end %>
<% end %>
<% content_for :head do %>
  <meta name="robots" content="noindex">
<% end %>

<% content_for :escape_link do %>
  <% if @presenter.show_escape_link? %>
    <%= render "smart_answers/shared/escape-link" %>
  <% end %>
<% end %>

<%= render partial: "smart_answers/outcome/#{outcome.partial_template_name}",
           locals: { outcome: outcome } %>
